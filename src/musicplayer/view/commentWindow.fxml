<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="anchorpane"  prefHeight="290.0" prefWidth="418.0" styleClass="greyBackground"  xmlns:fx="http://javafx.com/fxml/1" fx:controller="musicplayer.controller.CommentWindowController">
   <children>
      <TextArea fx:id="txtAreaComment" layoutX="12.0" layoutY="70.0" prefHeight="175.0" prefWidth="395.0" styleClass="text-area" stylesheets="@../css/darkAndGloomy.css" wrapText="true" />
      <Label fx:id="lblTitle" layoutX="14.0" layoutY="23.0" styleClass="mainLabelColor" stylesheets="@../css/darkAndGloomy.css" text="Label">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="btnSend" layoutX="348.0" layoutY="252.0" mnemonicParsing="false" onAction="#onBtnSendPressed"  text="Send -&gt;">
         <styleClass>
            <String fx:value="buttonColor" />
            <String fx:value="labelColor" />
         </styleClass></Button>
   </children>
</AnchorPane>
