         </styleClass></Button>
      <Button fx:id="modifySongButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="175.0" stylesheets="@../css/styleSheet.css" text="Modify song">
         <styleClass>
            <String fx:value="buttonColor" />
