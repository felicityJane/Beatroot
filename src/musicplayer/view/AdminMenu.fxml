            <String fx:value="buttonColor" />
            <String fx:value="labelColor" />
         </styleClass></Button>
      <Button fx:id="createAlbumButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="305.0" stylesheets="@../css/styleSheet.css" text="Create album">
