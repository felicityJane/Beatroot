<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="anchorpane"  prefHeight="152.0" prefWidth="362.0"  xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="musicplayer.controller.FriendRequestWindowController">
   <children>
      <ImageView fx:id="imgContactPicture" fitHeight="47.0" fitWidth="47.0" layoutX="50.0" layoutY="27.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="lblDisplayNameAndText" layoutX="108.0" layoutY="38.0" styleClass="mainLabelColor" stylesheets="@../css/darkAndGloomy.css" text="Label" wrapText="true" />
      <Button fx:id="btnAccept" layoutX="66.0" layoutY="98.0" mnemonicParsing="false" onAction="#onBtnAcceptPressed" prefHeight="25.0" prefWidth="83.0"  text="Accept">
         <styleClass>
            <String fx:value="buttonColor" />
            <String fx:value="labelColor" />
         </styleClass></Button>
      <Button fx:id="btnDecline" layoutX="211.0" layoutY="98.0" mnemonicParsing="false" onAction="#onBtnDeclinePressed" prefHeight="25.0" prefWidth="83.0"  text="Decline">
         <styleClass>
            <String fx:value="buttonColor" />
            <String fx:value="labelColor" />
         </styleClass></Button>
   </children>
</AnchorPane>
