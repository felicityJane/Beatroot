<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="settingsAnchorPage" prefHeight="336.0" prefWidth="471.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="musicplayer.controller.SettingsPageController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="159.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/beatrootlogo.png" />
         </image>
      </ImageView>
      <Button fx:id="backButton" layoutX="492.0" layoutY="37.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="74.0" text="Back" />
      <Label focusTraversable="false" layoutX="59.0" layoutY="120.0" text="Edit DisplayName" />
      <TextField fx:id="editDisplayNameField" layoutX="177.0" layoutY="116.0" />
      <Label fx:id="editDisplayNameLabel" focusTraversable="false" layoutX="385.0" layoutY="120.0" prefHeight="17.0" prefWidth="156.0" text="Label" />
      <Label focusTraversable="false" layoutX="69.0" layoutY="179.0" text="Edit Password" />
      <TextField fx:id="editPasswordField" layoutX="177.0" layoutY="175.0" />
      <Label fx:id="editPasswordLabel" focusTraversable="false" layoutX="385.0" layoutY="179.0" prefHeight="17.0" prefWidth="156.0" text="Label" />
      <Separator layoutX="52.0" layoutY="228.0" prefHeight="0.0" prefWidth="464.0" />
      <Label focusTraversable="false" layoutX="45.0" layoutY="264.0" text="Background Colour Settings" />
      <Button fx:id="backgroundBlackButton" layoutX="237.0" layoutY="260.0" mnemonicParsing="false" onAction="#onButton" prefHeight="25.0" prefWidth="48.0" style="-fx-background-color: black;" text="Black" textFill="#fcf9f9" />
      <Button fx:id="backgroundWhiteButton" layoutX="302.0" layoutY="260.0" mnemonicParsing="false" onAction="#offButton" text="White" />
      <Button fx:id="saveChangesButton" layoutX="350.0" layoutY="338.0" mnemonicParsing="false" onAction="#editDisplayName" text="Save Changes" />
      <fx:include fx:id="mainMenu" source="mainMenu.fxml" />
   </children>
</AnchorPane>
